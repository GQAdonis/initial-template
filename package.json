{
 "name": "one-app",
 "private": true,
 "version": "0.1.0",
 "type": "module",
 "scripts": {
  "dev": "vite",
  "dev:tauri": "tauri dev",
  "dev:ios": "cap sync ios && cap open ios",
  "dev:android": "cap sync android && cap open android",
  "dev:node": "./scripts/run-with-node-debug.sh",
  "dev:node:debug": "cd apps/node-ai-service && OPENAI_API_KEY=${OPENAI_API_KEY} node --inspect main.js",
  "dev:deno": "./scripts/run-with-deno-debug.sh",
  "dev:deno:debug": "OPENAI_API_KEY=${OPENAI_API_KEY} deno run --inspect --allow-all apps/ai-service/main.ts",
  "dev:axum": "./scripts/build-for-axum.sh && cd app/webserver && cargo run",
  "build": "tsc && vite build",
  "build:tauri": "yarn build:node-sidecar && tauri build",
  "build:tauri:windows": "yarn build:node-sidecar && tauri build --target windows",
  "build:tauri:windows:cross": "yarn build:node-sidecar && ./scripts/build-windows.sh --cross-compile",
  "build:ios": "tsc && vite build && cap sync ios",
  "build:android": "tsc && vite build && cap sync android",
  "build:axum": "./scripts/build-for-axum.sh",
  "build:node-sidecar": "./scripts/build-node-sidecar.sh",
  "preview": "vite preview",
  "preview:tauri": "tauri build --debug && tauri/target/debug/one-app",
  "cap": "cap",
  "tauri": "tauri"
 },
