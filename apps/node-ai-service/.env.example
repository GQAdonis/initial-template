# OpenAI API Key (required for both services)
OPENAI_API_KEY=your_openai_api_key_here

# API Service Port (default: 3000)
API_PORT=3000

# Mastra Agent Service Port (default: 4111)
AGENT_PORT=4111

# Mastra Base URL (default: http://localhost:4111)
MASTRA_BASE_URL=http://localhost:4111

# Node Environment
NODE_ENV=development